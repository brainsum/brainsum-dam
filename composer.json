{
    "name": "brainsum/brainsum-dam",
    "description": "Codebase for the brainsum DAM instance.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "mhavelant",
            "email": "mhavelant@brainsum.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "drupal-8": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "dropzone": {
            "type": "package",
            "package": {
                "name": "enyo-drupal/dropzone",
                "version": "v4.3.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
                    "type": "zip"
                }
            }
        }
    },
    "require": {
        "php": "^7.2",
        "brainsum/damo-distribution": ">=0.5 <1.0",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/views_bulk_operations": "^3.8",
        "drush/drush": "^9.0|^10.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "symfony/event-dispatcher": "4.3.4 as 3.4.35",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "drupal-composer/drupal-security-advisories": "8.x-dev",
        "drupal/console": "^1.9",
        "drupal/core-dev": "^8.8",
        "drupal/devel": "^4.0",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/devel_php": "^1.1",
        "drupal/upgrade_status": "^2.9",
        "roave/security-advisories": "dev-master",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "autoload": {
        "files": [
            "load.environment.php"
        ]
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/google_cloud_vision": {
                "S3FS compatibility of fopen().": "https://www.drupal.org/files/issues/2020-05-25/fopen-user-error-when-using-s3fs-13644864-3.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[web-root]/sites/default/default.settings.php": false,
                "[web-root]/sites/example.settings.local.php": false,
                "[web-root]/sites/default/default.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        }
    }
}
